{
  "name": "uploadsql",
  "version": "1.1.2",
  "main": "main.js",
  "scripts": {
    "dev:backend": "cd backend && npm run start",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:electron": "electron .",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:electron\"",
    "build:frontend": "cd frontend && npm run build",
    "build": "npm run build:frontend && electron-builder",
    "build:win": "npm run build:frontend && electron-builder --win",
    "dist": "npm run build"
  },
  "build": {
    "appId": "com.santiago.uploadsql",
    "productName": "UploadSql",
    "icon": "build/icon.png",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "backend/**/*",
      "frontend/dist/**/*"
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Development"
    },
    "win": {
      "target": "nsis"
    }
  },
  "keywords": [],
  "author": {
    "name": "Santiago",
    "email": "santiago@uploadsql.com"
  },
  "homepage": "https://github.com/santiago/uploadsql",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "electron": "^40.6.0",
    "electron-builder": "^26.8.1"
  }
}